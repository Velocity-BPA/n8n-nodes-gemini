{
  "name": "n8n-nodes-gemini",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Gemini cryptocurrency exchange providing 11 resources and 80+ operations for trading, staking, earn, and account management. Includes spot trading, perpetual futures, GUSD support, and master/sub-account management.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-gemini.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-gemini#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-gemini/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "gemini",
    "cryptocurrency",
    "exchange",
    "trading",
    "crypto",
    "bitcoin",
    "ethereum",
    "winklevoss",
    "gusd",
    "staking",
    "defi",
    "blockchain",
    "api"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/GeminiApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Gemini/Gemini.node.js",
      "dist/nodes/Gemini/GeminiTrigger.node.js"
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.27.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
